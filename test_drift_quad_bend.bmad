parameter[lattice] = test_drift_quad_bend

beginning[beta_a] = 10
beginning[beta_b] = 10

beginning[p0c] = 4.0e7
parameter[particle] = electron
parameter[geometry] = open

d1: drift,  L = 1.0
q1: quad, L = 0.1, K1 = 10.0
d2: drift, L = 0.3 
q2: quad, L = 0.1 , K1 = -10.0 
d3: drift,  L = 0.3 
b1: sbend, rho = 1.0, angle = 20 * pi/180
d4: drift, L = 1.0

lat: line = (d1, q1, d2, q2, d3, b1, d4)

use, lat
